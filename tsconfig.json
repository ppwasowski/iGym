{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "/components/**/*.tsx",
    "/screens/**/*.tsx",
    "App.js",
    "my-app.d.ts",
    "App/**/*.tsx",],
}
